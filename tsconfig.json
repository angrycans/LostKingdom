{
  "compilerOptions": {
    "module": "commonjs",
    "lib": [
      "dom",
      "es5",
      "es2016",
      "es2015.promise",
      "es2017"
    ],
    "target": "es6",
    "allowJs": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "baseUrl": "./assets/Script",
    "outDir": "./dist/",
  },
  "exclude": [
    "node_modules",
    "library",
    "local",
    "temp",
    "build",
    "settings"
  ]
}